{
  "name": "vscode-end-of-line",
  "displayName": "End Of Line",
  "description": "Insert or Select a string at the end of a line.",
  "version": "1.0.0",
  "publisher": "SatoshiYamamoto",
  "author": {
    "name": "Satoshi Yamamoto",
    "email": "standard.software.net@gmail.com",
    "url": "https://www.facebook.com/stndardsoftware"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/standard-software/vscode-end-of-line"
  },
  "engines": {
    "vscode": "^1.62.1"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "keywords": [
    "indent",
    "reply mark",
    "template"
  ],
  "activationEvents": [
    "onCommand:EndOfLine.Input",
    "onCommand:EndOfLine.SelectEdit"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "EndOfLine.Input",
        "title": "End Of Line | Input"
      },
      {
        "command": "EndOfLine.SelectEdit",
        "title": "End Of Line | Select Edit"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "End Of Line",
      "properties": {
        "EndOfLine.insertString": {
          "type": "string",
          "default": "> ",
          "description": "Insert String"
        }
      },
      "default": {
        "insertString": "> "
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "graphemesplit": "^2.4.4"
  }
}
